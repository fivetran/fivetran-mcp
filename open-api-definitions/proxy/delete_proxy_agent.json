{
  "openapi": "3.0.1",
  "info": {
    "title": "Delete a Proxy Agent",
    "description": "Deletes the specified proxy agent from your Fivetran account."
  },
  "path": "/v1/proxy/{agentId}",
  "method": "DELETE",
  "operation": {
    "tags": [
      "Proxy Agent Management"
    ],
    "summary": "Delete a Proxy Agent",
    "description": "Deletes the specified proxy agent from your Fivetran account.",
    "operationId": "delete_proxy_agent",
    "parameters": [
      {
        "name": "agentId",
        "in": "path",
        "description": "The unique identifier for the proxy agent within the Fivetran system.",
        "required": true,
        "schema": {
          "type": "string",
          "example": "agentId"
        },
        "example": "agentId"
      },
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "message"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                }
              }
            },
            "examples": {
              "delete_successful": {
                "value": {
                  "code": "Success",
                  "message": "Agent <proxy> successfully deleted."
                }
              }
            }
          }
        }
      },
      "404": {
        "description": "Proxy agent not found.",
        "content": {
          "application/json": {
            "examples": {
              "proxy_not_found": {
                "value": {
                  "code": "NotFound",
                  "message": "Proxy Agent not found."
                }
              }
            }
          }
        }
      },
      "409": {
        "description": "Proxy agent have attached connections.",
        "content": {
          "application/json": {
            "examples": {
              "proxy_has_connections": {
                "value": {
                  "code": "Conflict",
                  "message": "Proxy agent have attached connections."
                }
              }
            }
          }
        }
      }
    }
  }
}
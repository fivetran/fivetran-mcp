{
  "openapi": "3.0.1",
  "info": {
    "title": "Retrieve Proxy Agent Details",
    "description": "Retrieves the details of the specified proxy agent."
  },
  "path": "/v1/proxy/{agentId}",
  "method": "GET",
  "operation": {
    "tags": [
      "Proxy Agent Management"
    ],
    "summary": "Retrieve Proxy Agent Details",
    "description": "Retrieves the details of the specified proxy agent.",
    "operationId": "get_proxy_agent_details",
    "parameters": [
      {
        "name": "agentId",
        "in": "path",
        "description": "The unique identifier for the proxy agent within the Fivetran system.",
        "required": true,
        "schema": {
          "type": "string",
          "example": "agentId"
        },
        "example": "agentId"
      },
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "$ref": "#/components/schemas/ProxyAgentResponse"
            },
            "examples": {
              "get_detail": {
                "value": {
                  "code": "Success",
                  "data": {
                    "id": "id",
                    "account_id": "account_id",
                    "registered_at": "2018-12-01T15:43:29.013729Z",
                    "region": "GCP_US_EAST4",
                    "created_by": "created_by",
                    "display_name": "display_name"
                  }
                }
              }
            }
          }
        }
      },
      "404": {
        "description": "Proxy Agent not found",
        "content": {
          "application/json": {
            "examples": {
              "proxy_not_found": {
                "value": {
                  "code": "NotFound",
                  "message": "Proxy Agent not found."
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "ProxyAgentResponse": {
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique identifier for the proxy agent within the Fivetran system.",

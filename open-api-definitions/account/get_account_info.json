{
  "openapi": "3.0.1",
  "info": {
    "title": "Get Account Info from the API Key",
    "description": "Returns information about current account from API key."
  },
  "path": "/v1/account/info",
  "method": "GET",
  "operation": {
    "tags": [
      "Account Management"
    ],
    "summary": "Get Account Info from the API Key",
    "description": "Returns information about current account from API key.",
    "operationId": "get_account_info",
    "parameters": [
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "data"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                },
                "data": {
                  "$ref": "#/components/schemas/AccountInfoResponse"
                }
              }
            }
          }
        }
      },
      "401": {
        "description": "Invalid authorization credentials"
      }
    }
  },
  "components": {
    "schemas": {
      "AccountInfoResponse": {
        "required": [
          "account_id"
        ],
        "type": "object",
        "properties": {
          "account_id": {
            "type": "string",
            "description": "The unique identifier for the account within the Fivetran system.",
            "example": "account_id"
          },
          "system_key_id": {
            "type": "string",
            "description": "The system ID from the API key. Returned if the system API key is used to authenticate the request.",
            "example": "system_key_id"
          },
          "account_name": {
            "type": "string"
          },
          "user_id": {
            "type": "string",
            "description": "The user ID from the API key. Returned if the user API key is used to authenticate the request.",
            "example": "user_id"
          }
        }
      }
    }
  }
}
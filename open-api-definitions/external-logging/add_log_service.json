{
  "openapi": "3.0.1",
  "info": {
    "title": "Create a Log Service",
    "description": "Creates a new logging service within a specified group in your Fivetran account."
  },
  "path": "/v1/external-logging",
  "method": "POST",
  "operation": {
    "tags": [
      "Log Service Management"
    ],
    "summary": "Create a Log Service",
    "description": "Creates a new logging service within a specified group in your Fivetran account.",
    "operationId": "add_log_service",
    "parameters": [
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/AddLogRequest"
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "data"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                },
                "data": {
                  "$ref": "#/components/schemas/LogExtendedResponse"
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "LogExtendedResponse": {
        "required": [
          "enabled",
          "id",
          "service"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique identifier for the log service within the Fivetran system.",
            "example": "log_id"
          },
          "service": {
            "type": "string",
            "description": "The name of the log service type within the Fivetran system. We support the following log services: `azure_monitor_log`, `cloudwatch`, `datadog_log`, `splunkLog`, `new_relic_log`, `stackdriver` (Google Cloud Logging).",
            "example": "log_service_type"
          },
          "enabled": {
            "type": "boolean",
            "description": "The boolean value specifying whether the log service is enabled.",
            "example": true
          }
        },
        "discriminator": {
          "propertyName": "service",
          "mapping": {
            "azure_monitor_log": "#/components/schemas/azure_monitor_log_LogExtendedResponse",
            "cloudwatch": "#/components/schemas/cloudwatch_LogExtendedResponse",
            "datadog_log": "#/components/schemas/datadog_log_LogExtendedResponse",
            "new_relic_log": "#/components/schemas/new_relic_log_LogExtendedResponse",
            "splunkLog": "#/components/schemas/splunkLog_LogExtendedResponse",
            "stackdriver": "#/components/schemas/stackdriver_LogExtendedResponse"
          }
        }
      },
      "AddLogRequest": {
        "type": "object",
        "properties": {
          "group_id": {
            "type": "string",
            "description": "The unique identifier for the group within the Fivetran system",
            "example": "group_id"
          },
          "service": {
            "type": "string",
            "description": "The name of the log service type within the Fivetran system. We support the following log services: `azure_monitor_log`, `cloudwatch`, `datadog_log`, `splunkLog`, `new_relic_log`, `stackdriver` (Google Cloud Logging).",
            "example": "log_service_type"
          },
          "enabled": {
            "type": "boolean",
            "description": "The boolean value specifying whether the log service is enabled.",
            "example": true
          }
        },
        "discriminator": {
          "propertyName": "service",
          "mapping": {
            "azure_monitor_log": "#/components/schemas/azure_monitor_log_AddLogRequest",
            "cloudwatch": "#/components/schemas/cloudwatch_AddLogRequest",
            "datadog_log": "#/components/schemas/datadog_log_AddLogRequest",
            "new_relic_log": "#/components/schemas/new_relic_log_AddLogRequest",
            "splunkLog": "#/components/schemas/splunkLog_AddLogRequest",
            "stackdriver": "#/components/schemas/stackdriver_AddLogRequest"
          }
        }
      }
    }
  }
}
{
  "openapi": "3.0.1",
  "info": {
    "title": "Update a Private Link",
    "description": "Updates information for an existing private link within your Fivetran account."
  },
  "path": "/v1/private-links/{privateLinkId}",
  "method": "PATCH",
  "operation": {
    "tags": [
      "Private Link Management"
    ],
    "summary": "Update a Private Link",
    "description": "Updates information for an existing private link within your Fivetran account.",
    "operationId": "modify_private_link",
    "parameters": [
      {
        "name": "privateLinkId",
        "in": "path",
        "description": "The unique identifier for the private link within the Fivetran system",
        "required": true,
        "schema": {
          "type": "string",
          "example": "privateLinkId"
        },
        "example": "privateLinkId"
      },
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/PrivateLinkUpdateRequest"
          }
        }
      }
    },
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "data"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                },
                "data": {
                  "$ref": "#/components/schemas/PrivateLinkResponse"
                }
              }
            }
          }
        }
      },
      "404": {
        "description": "Private Link not found"
      }
    }
  },

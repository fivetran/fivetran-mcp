      },
      "SetupTestResultResponse": {
        "required": [
          "status",
          "title"
        ],
        "type": "object",
        "properties": {
          "title": {
            "type": "string",
            "description": "Setup test title",
            "example": "Test Title"
          },
          "status": {
            "type": "string",
            "description": "Setup test status",
            "example": "FAILED",
            "enum": [
              "PASSED",
              "SKIPPED",
              "WARNING",
              "FAILED",
              "JOB_FAILED"
            ]
          },
          "message": {
            "type": "string",

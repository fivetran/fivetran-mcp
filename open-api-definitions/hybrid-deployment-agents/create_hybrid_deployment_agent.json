{
  "openapi": "3.0.1",
  "info": {
    "title": "Create a Hybrid Deployment Agent",
    "description": "Creates a new Hybrid Deployment Agent in a group."
  },
  "path": "/v1/hybrid-deployment-agents",
  "method": "POST",
  "operation": {
    "tags": [
      "Hybrid Deployment Agent Management"
    ],
    "summary": "Create a Hybrid Deployment Agent",
    "description": "Creates a new Hybrid Deployment Agent in a group.",
    "operationId": "create_hybrid_deployment_agent",
    "parameters": [
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "requestBody": {
      "content": {
        "application/json": {
          "schema": {
            "$ref": "#/components/schemas/HybridDeploymentAgentCreateRequest"
          }
        }
      }
    },
    "responses": {
      "201": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "data"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                },
                "data": {
                  "$ref": "#/components/schemas/HybridDeploymentAgentCreateResponse"
                }
              }
            }
          }
        }
      },
      "400": {
        "description": "Missing name or license not accepted in the payload."
      },
      "404": {
        "description": "Group not found."
      }
    }
  },
  "components": {
    "schemas": {
      "HybridDeploymentAgentCreateRequest": {
        "required": [
          "accept_terms",
          "display_name",
          "env_type"
        ],
        "type": "object",
        "properties": {
          "accept_terms": {
            "type": "boolean",
            "description": "Boolean, must be true to indicate that the caller accepts the Fivetran On-Prem Software License Addendum.",
            "example": true
          },
          "auth_type": {
            "type": "string",
            "description": "Agent Auth Mode. Defines how agent should be authorised.",
            "example": "AUTO",
            "enum": [
              "AUTO",
              "MANUAL"
            ]
          },
          "display_name": {
            "type": "string",
            "description": "Hybrid Deployment Agent display name.",
            "example": "display_name"
          },
          "env_type": {
            "type": "string",
            "description": "Environment type.",
            "example": "DOCKER",
            "enum": [
              "DOCKER",
              "PODMAN",
              "KUBERNETES",
              "SNOWPARK"
            ]
          },
          "group_id": {
            "type": "string",
            "description": "The unique identifier for the group within your Fivetran account.",
            "example": "group_id"
          }
        }
      },
      "HybridDeploymentAgentCreateResponse": {
        "required": [
          "created_by",
          "deployment_type",
          "display_name",
          "enabled",
          "group_id",
          "id",
          "last_used_at",
          "online",
          "registered_at",
          "updated_at",
          "version"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique identifier for the hybrid deployment agent within the Fivetran system.",
            "example": "agent_id"
          },
          "version": {
            "type": "string",
            "description": "Version of the Hybrid Deployment Agent",
            "example": "version"
          },
          "enabled": {
            "type": "boolean",
            "description": "The boolean value specifying whether the Hybrid Deployment Agent is enabled.",
            "example": true
          },
          "online": {
            "type": "boolean",
            "description": "The boolean value specifying whether the Hybrid Deployment Agent is online.",
            "example": true
          },
          "files": {
            "$ref": "#/components/schemas/HybridDeploymentAgentFilesResponse"
          },
          "token": {
            "type": "string",
            "description": "Base64 encoded content of token",
            "example": "token_value"
          },
          "display_name": {
            "type": "string",
            "description": "Hybrid Deployment Agent display name.",
            "example": "display_name"
          },
          "group_id": {
            "type": "string",
            "description": "The unique identifier for the group within your Fivetran account.",
            "example": "group_id"
          },
          "registered_at": {
            "type": "string",
            "description": "Time when this Hybrid Deployment Agent was created.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          },
          "deployment_type": {
            "type": "string",
            "description": "Environment type.",
            "example": "DOCKER",
            "enum": [
              "DOCKER",
              "PODMAN",
              "KUBERNETES",
              "SNOWPARK"
            ]
          },
          "created_by": {
            "type": "string",
            "description": "The actor who created the Hybrid Deployment Agent",
            "example": "created_by"
          },
          "last_used_at": {
            "type": "string",
            "description": "The time this Hybrid Deployment Agent was last used.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          },
          "updated_at": {
            "type": "string",
            "description": "Time when this Hybrid Deployment Agent was updated.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          }
        }
      },
      "HybridDeploymentAgentFilesResponse": {
        "required": [
          "auth_json",
          "config_json",
          "docker_compose_yaml"
        ],
        "type": "object",
        "properties": {
          "config_json": {
            "type": "string",
            "description": "Base64 encoded content of the config.json",
            "format": "byte",
            "example": "Base64 {... config.json contents ...}"
          },
          "docker_compose_yaml": {
            "type": "string",
            "description": "Base64 encoded content of the docker-compose.yaml",
            "format": "byte",
            "example": "Base64 {... docker-compose.yaml contents ...}"
          },
          "auth_json": {
            "type": "string",
            "description": "Base64 encoded content of the auth.json",
            "format": "byte",
            "example": "Base64 {... auth.json contents ...}"
          }
        }
      }
    }
  }
}
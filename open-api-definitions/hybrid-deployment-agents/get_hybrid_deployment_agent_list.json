{
  "openapi": "3.0.1",
  "info": {
    "title": "List Hybrid Deployment Agents",
    "description": "Returns list of all Hybrid Deployment Agents within your Fivetran account, along with usage. Optionally filtered to a single group."
  },
  "path": "/v1/hybrid-deployment-agents",
  "method": "GET",
  "operation": {
    "tags": [
      "Hybrid Deployment Agent Management"
    ],
    "summary": "List Hybrid Deployment Agents",
    "description": "Returns list of all Hybrid Deployment Agents within your Fivetran account, along with usage. Optionally filtered to a single group.",
    "operationId": "get_hybrid_deployment_agent_list",
    "parameters": [
      {
        "name": "groupId",
        "in": "query",
        "description": "The Fivetran Group Id.",
        "required": false,
        "schema": {
          "type": "string",
          "example": "group_id"
        },
        "example": "group_id"
      },
      {
        "name": "cursor",
        "in": "query",
        "schema": {
          "type": "string"
        }
      },
      {
        "name": "limit",
        "in": "query",
        "schema": {
          "type": "integer",
          "format": "int32"
        }
      },
      {
        "name": "Accept",
        "in": "header",
        "schema": {
          "type": "string",
          "example": "application/json",
          "default": "application/json"
        }
      }
    ],
    "responses": {
      "200": {
        "description": "Successful response",
        "content": {
          "application/json": {
            "schema": {
              "required": [
                "code",
                "data"
              ],
              "properties": {
                "code": {
                  "type": "string",
                  "description": "Response status code",
                  "example": "Success"
                },
                "message": {
                  "type": "string",
                  "description": "Response status text",
                  "example": "Operation performed."
                },
                "data": {
                  "required": [
                    "items"
                  ],
                  "properties": {
                    "items": {
                      "type": "array",
                      "description": "The collection of returned items",
                      "items": {
                        "$ref": "#/components/schemas/HybridDeploymentAgentWithUsageResponse"
                      }
                    },
                    "next_cursor": {
                      "type": "string",
                      "description": "The value of the cursor parameter for the next page",
                      "example": "cursor_value"
                    }
                  }
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "HybridDeploymentAgentWithUsageResponse": {
        "required": [
          "created_by",
          "deployment_type",
          "display_name",
          "enabled",
          "group_id",
          "id",
          "last_used_at",
          "online",
          "registered_at",
          "updated_at",
          "version"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "description": "The unique identifier for the hybrid deployment agent within the Fivetran system.",
            "example": "agent_id"
          },
          "version": {
            "type": "string",
            "description": "Version of the Hybrid Deployment Agent",
            "example": "version"
          },
          "enabled": {
            "type": "boolean",
            "description": "The boolean value specifying whether the Hybrid Deployment Agent is enabled.",
            "example": true
          },
          "online": {
            "type": "boolean",
            "description": "The boolean value specifying whether the Hybrid Deployment Agent is online.",
            "example": true
          },
          "usage": {
            "type": "array",
            "items": {
              "$ref": "#/components/schemas/HybridDeploymentAgentUsageResponse"
            }
          },
          "display_name": {
            "type": "string",
            "description": "Hybrid Deployment Agent display name.",
            "example": "display_name"
          },
          "group_id": {
            "type": "string",
            "description": "The unique identifier for the group within your Fivetran account.",
            "example": "group_id"
          },
          "registered_at": {
            "type": "string",
            "description": "Time when this Hybrid Deployment Agent was created.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          },
          "deployment_type": {
            "type": "string",
            "description": "Environment type.",
            "example": "DOCKER",
            "enum": [
              "DOCKER",
              "PODMAN",
              "KUBERNETES",
              "SNOWPARK"
            ]
          },
          "created_by": {
            "type": "string",
            "description": "The actor who created the Hybrid Deployment Agent",
            "example": "created_by"
          },
          "last_used_at": {
            "type": "string",
            "description": "The time this Hybrid Deployment Agent was last used.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          },
          "updated_at": {
            "type": "string",
            "description": "Time when this Hybrid Deployment Agent was updated.",
            "format": "date-time",
            "example": "2024-12-01T15:43:29.013729Z"
          }
        }
      },
      "HybridDeploymentAgentUsageResponse": {
        "required": [
          "connection_id",
          "schema",
          "service"
        ],
        "type": "object",
        "properties": {
          "schema": {
            "type": "string",
            "description": "The name used both as the connection's name within the Fivetran system and as the source schema's name within your group.",
            "example": "schema_name"
          },
          "service": {
            "type": "string",
            "description": "The connector type name within the Fivetran system.",
            "example": "service"
          },
          "connection_id": {
            "type": "string",
            "description": "The unique identifier for the connection within the Fivetran",
            "example": "connection_id"
          }
        }
      }
    }
  }
}